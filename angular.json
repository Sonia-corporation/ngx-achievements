{
  "version": 1,
  "projects": {
    "website": {
      "projectType": "application",
      "root": "apps/website",
      "sourceRoot": "apps/website/src",
      "prefix": "ngx-achievements",
      "i18n": {
        "sourceLocale": "en-US",
        "locales": {
          "fr": "apps/website/src/locale/messages.fr.xlf",
          "en": "apps/website/src/locale/messages.en.xlf"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/website",
            "index": "apps/website/src/index.html",
            "main": "apps/website/src/main.ts",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": ["apps/website/src/favicon.ico", "apps/website/src/assets"],
            "styles": ["apps/website/src/styles.scss"],
            "scripts": [],
            "aot": true,
            "buildOptimizer": true,
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": true,
            "progress": true,
            "extractLicenses": true,
            "verbose": false,
            "statsJson": false,
            "vendorChunk": true,
            "commonChunk": true,
            "i18nMissingTranslation": "error",
            "localize": ["fr", "en"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "localize": ["fr", "en"]
            },
            "fr": {
              "buildOptimizer": false,
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "extractLicenses": false,
              "localize": ["fr"],
              "baseHref": "fr"
            },
            "en": {
              "buildOptimizer": false,
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "extractLicenses": false,
              "localize": ["en"],
              "baseHref": "en"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "website:build:production"
            },
            "fr": {
              "browserTarget": "website:build:fr"
            },
            "en": {
              "browserTarget": "website:build:en"
            }
          },
          "defaultConfiguration": "en"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "website:build",
            "outputPath": "apps/website/src/locale",
            "outFile": "messages.en.xlf"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/website/src/**/*.ts", "apps/website/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/website"],
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "apps/website/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/website",
            "config": {
              "configFolder": "apps/website/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "watch-storybook": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["npx nx run website:compodoc:json-watch", "npx nx run website:storybook"],
            "parallel": true
          }
        },
        "build-storybook-with-docs": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["npx nx run website:compodoc:json", "npx nx run website:build-storybook"],
            "parallel": false
          }
        },
        "compodoc": {
          "builder": "@twittwer/compodoc:compodoc",
          "options": {
            "tsConfig": "apps/website/tsconfig.editor.json",
            "outputPath": "dist/compodoc/website",
            "disableCoverage": false,
            "disablePrivate": false,
            "disableInternal": false
          },
          "configurations": {
            "json": {
              "exportFormat": "json"
            },
            "json-watch": {
              "exportFormat": "json",
              "watch": true
            }
          }
        }
      }
    },
    "website-e2e": {
      "root": "apps/website-e2e",
      "sourceRoot": "apps/website-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/website-e2e/cypress.json",
            "tsConfig": "apps/website-e2e/tsconfig.e2e.json",
            "devServerTarget": "website:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "website:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/website-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "58530ef3-3bee-4b75-8655-527d16dc6f4a",
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "website"
}
